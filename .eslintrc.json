{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard-with-typescript",
        "plugin:vue/vue3-recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "parser": "@typescript-eslint/parser",
        "project": "./tsconfig.json",
        "extraFileExtensions": [
            ".vue"
        ]
    },
    "plugins": [
        "vue"
    ],
    "rules": {},
    "ignorePatterns": [
        "dist",
        "node_modules",
        ".eslintrc.cjs",
        "vite.config.ts",
        "package.json"
    ]
}